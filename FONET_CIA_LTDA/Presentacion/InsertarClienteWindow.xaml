        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
        mc:Ignorable="d"
        Title="INSERTAR CLIENTE" Height="389.407" Width="546.237"  ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Height="364" Margin="0,0,0,-4" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition Height="323*"/>
