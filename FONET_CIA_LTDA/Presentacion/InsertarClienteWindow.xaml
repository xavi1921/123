        <Button x:Name="btnCancelar" Style="{DynamicResource ResourceKey=MaterialDesignFlatButton}" Content="Cancelar" HorizontalAlignment="Left" Margin="148,301,0,0" VerticalAlignment="Top" Click="BtnCancelar_Click" Foreground="DarkRed" Grid.RowSpan="2"/>
        <Label x:Name="lblTitulo" Content="INSERTAR" HorizontalAlignment="Left" Margin="34,11,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtDireccion"  materialDesign:HintAssist.Hint="Dirección" materialDesign:HintAssist.IsFloating="True" Margin="249,62,84,208.468" MaxLength="50"/>
        <TextBox x:Name="txtCorreo"  materialDesign:HintAssist.Hint="Correo" materialDesign:HintAssist.IsFloating="True" Margin="251,168,84,103.468" MaxLength="50"/>
        <TextBox x:Name="txtTelefon"  materialDesign:HintAssist.Hint="Teléfono" materialDesign:HintAssist.IsFloating="True" Margin="250,115,84,155.468" MaxLength="10" InputScope="Number" PreviewTextInput="TxtTelefon_PreviewTextInput"/>
        <TextBox x:Name="txtReferencia"  materialDesign:HintAssist.Hint="Referebcua" materialDesign:HintAssist.IsFloating="True" Margin="37,225,306,46.468" MaxLength="50"/>
        <TextBox x:Name="txtCoordenadas"  materialDesign:HintAssist.Hint="Coordenadas" materialDesign:HintAssist.IsFloating="True" Margin="246,225,84,46.468" MaxLength="50"/>
        <TextBox x:Name="txtNrContratoy" IsReadOnly="False" materialDesign:HintAssist.Hint="# Contrato" materialDesign:HintAssist.IsFloating="True" Margin="323,8,10,261.468" MaxLength="10" InputScope="Number" PreviewTextInput="txtCedula_PreviewTextInput"/>
    </Grid>
</Window>
